{
    "name": "perfumer/upload",
    "type": "library",
    "description": "File upload server",
    "homepage": "https://github.com/perfumer/upload",
    "authors": [
        {
            "name": "Ilyas Makashev",
            "email": "mehmatovec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "perfumer/framework": "0.5.3",
        "propel/propel": "2.0.0-alpha8",
        "pwfisher/command-line-php": "dev-master",
        "symfony/console": "4.1.6",
        "symfony/http-foundation": "4.2.12",
        "imagine/imagine": "0.6.3",
        "nikic/fast-route": "1.2.0",
        "codeguy/upload": "1.3.2"
    },
    "require-dev": {
        "filp/whoops": "2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Upload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
